{
  "name": "e-ordering",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "core/pwa-studio",
    "core/pwa-studio/packages/*",
    "core/pwa-studio/packages/extensions/*",
    "cto-packages/*"
  ],
  "scripts": {
    "build": "yarn workspace @magento/pwa-studio run build",
    "build:dev": "yarn workspace @magento/pwa-studio run build:dev",
    "watch": "yarn workspace @magento/pwa-studio run watch:venia",
    "watch:all": "yarn workspace @magento/pwa-studio run watch:all"
  },
  "peerDependencies": {
    "@magento/pwa-buildpack": "*",
    "@magento/venia-ui": "*",
    "react": "~16.9.0",
    "react-router-dom": "~5.1.0"
  }
}
